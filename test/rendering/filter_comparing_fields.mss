#world[[FOO] = [BAR]] {
  polygon-fill: #FFF;
  line-color:#F00;
  line-width: 0.5;
}

#world[[BAR] = [HI]] {
  polygon-fill: #FFF;
  line-color:#F00;
  line-width: 0.5;
}

#world[[BAR] = [HI]] {
  polygon-fill: #FFF;
  line-color:#F00;
  line-width: 5;
}

#world[[BAR] > [HI]] {
  polygon-fill: #FFF;
  line-color:#F00;
  line-width: 5;
}
